{
  "imports": {
    "~/": "./app/",
    "@hono/hono": "npm:hono@^4.10.3",
    "@std/http/": "npm:/@jsr/std__http@1.0.21/",
    "@std/path/": "npm:/@jsr/std__path@1.1.2/",
    "@std/crypto": "npm:@jsr/std__crypto@1.0.5",
    "@std/crypto/": "npm:/@jsr/std__crypto@1.0.5/",
    "@std/encoding": "npm:@jsr/std__encoding@1.0.10",
    "@std/encoding/": "npm:/@jsr/std__encoding@1.0.10/",
    "@std/dotenv/": "npm:/@jsr/std__dotenv@0.225.5/",
    "@supabase/supabase-js": "npm:@jsr/supabase__supabase-js@^2.58.0",
    "@libs/qrcode": "npm:@jsr/libs__qrcode@^3.0.0",
    "zod": "npm:zod@^3.25.76"
  },
  "tasks": {
    "lint": "deno lint",
    "dev": "DENO_TLS_CA_STORE=system deno run --inspect --env-file=.env --allow-read --allow-net --allow-sys=hostname --allow-env --watch ./dev.ts",
    "start": "deno run --allow-env --allow-net --allow-read --allow-sys=hostname ./main.ts",
    "build": "NODE_ENV=production react-router build",
    "format": "deno fmt",
    "typecheck": {
      "command": "deno check --node-modules-dir=auto '**/*'",
      "dependencies": [
        "typegen"
      ]
    },
    "typegen": "react-router typegen",
    "sync": "deno run --allow-net --allow-env --allow-write --allow-read ./jobs/worker.ts"
  },
  "compilerOptions": {
    "lib": [
      "deno.window",
      "dom"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react",
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "types": [
      "vite/client"
    ]
  },
  "unstable": [
    "sloppy-imports"
  ],
  "exclude": [
    "build/"
  ],
  "nodeModulesDir": "manual"
}
