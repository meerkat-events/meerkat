{
  "imports": {
    "@hono/hono": "npm:hono@^4.9.11",
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.7.4",
    "@libs/qrcode": "jsr:@libs/qrcode@^3.0.0",
    "@parcnet-js/ticket-spec": "npm:@parcnet-js/ticket-spec@^1.1.9",
    "@pcd/pod": "npm:@pcd/pod@^0.5.1",
    "@sentry/deno": "npm:@sentry/deno@^10.19.0",
    "@std/crypto": "jsr:@std/crypto@^1.0.5",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@supabase/supabase-js": "jsr:@supabase/supabase-js@^2.58.0",
    "drizzle-orm": "npm:drizzle-orm@^0.44.6",
    "pino": "npm:pino@^10.0.0",
    "postgres": "npm:postgres@^3.4.7",
    "qrcode": "npm:qrcode@^1.5.4",
    "uuidv7": "npm:uuidv7@^1.0.2",
    "zod": "npm:zod@^4.1.12",
    "drizzle-kit": "npm:drizzle-kit@^0.31.5"
  },
  "tasks": {
    "check": "deno check **/*.ts",
    "lint": "deno lint",
    "dev": "DENO_TLS_CA_STORE=system OTEL_DENO=true OTEL_SERVICE_NAME=api deno run --inspect --allow-env --unstable-otel --allow-read --allow-net --allow-sys --watch ./dev.ts",
    "start": "deno run --allow-env --allow-net --allow-read=public --allow-sys=hostname ./main.ts"
  }
}
